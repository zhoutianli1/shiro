<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhou.mapper.AccountMapper">

    <select id="queryAccByName" parameterType="String" resultType="Account">
        select * from mybatis.account where username=#{username}
    </select>
</mapper>